  },
  "homepage": "https://github.com/Emethium/nest-api-example",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "docdash": "^1.1.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
